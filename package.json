	"description": "Webview for bun based on tauri's tao and wry.",
	"keywords": [
		"corelauncher",
		"webview",
		"bun",
		"tauri",
		"tao",
		"wry"
	],
	"homepage": "https://github.com/CoreLauncher/rod#readme",
	"bugs": {
		"url": "https://github.com/CoreLauncher/rod/issues"
	},
	"license": "MIT",
	"author": "CoreByte",
	"files": [
		"LICENSE",
		"README.md",
		"/src-ts/*",
		"/target/release/rod.dll",
		"/target/release/librod.so"
	],
